<!--components/bookitem/bookitem.wxml-->
<view class='bookitem-container' bindtap='navigateToChapter'>
  <view class='header'>
    <text class='book-name'>{{book.bookName}}</text>
    <text class='update-time'>{{book.updateTime}}</text>
  </view>
  <view class='chapter-name'>{{book.newestCatalogName}}</view>
</view>